CREATE TABLE Characters (
	Character_ID int  auto_increment not null, Character_name varchar(50) not null,
    Character_class varchar(100) not  null, Character_level int not null,
    Character_xp int not null, Character_weapon varchar(100) not null,
    PRIMARY KEY(Character_ID));

CREATE TABLE Weapons (
    Weapon_id int auto_increment not null, Weapon_name varchar(100) not null, Weapon_rarity varchar(50) not null, Weapon_normalHits int not null,
    Weapon_specialHits int not null, weapon_chanceForSuperAbility int not null, primary key(Weapon_id));

CREATE TABLE usedCharacters (
    Character_ID int  auto_increment not null, Character_name varchar(50) not null,
    Character_class varchar(100) not null, Character_level int not null,
    Character_xp int not null, Character_weapon varchar(100) not null,
    PRIMARY KEY(Character_ID));

CREATE TABLE characterClass (
    character_id int auto_increment not null, character_name varchar(50) not null,
    character_hp int not null, character_ap int not null,
    character_lvl int not null, character_xp int not null,
    character_lvlReq int not null, character_hitmes1 varchar(200) not null,
    character_hitmes2 varchar(200) not null, character_hitmes3 varchar(200) not null,
    character_specialatt varchar(200) not null, PRIMARY KEY(character_id)
    );
CREATE TABLE pictureSrc (
    picture_id int auto_increment not null, picture_class varchar(100) not null,
    picture_src varchar(100) not null, primary key(picture_id)
    );
CREATE Table tournamentResults (
    tournament_id int auto_increment not null, winner varchar(50) not null,
    primary key(tournament_id)
    );
CREATE TABLE effects (
    effect_id int auto_increment not null, effect_name varchar(50) not null,
    effect_tier varchar(5) not null, effect_number int not null,
    primary key(effect_id)
    );


INSERT INTO `characterclass` VALUES (1,'Fighter',125,20,1,0,100,'%s hits %s and deals %d direct damage with single punch. Weapon: %s','%s hits %s and deals %d direct damage with drop kick. Weapon: %s','%s hits %s and deals %d direct damage with roundhouse kick. Weapon: %s','%s uses skill: Brave Warrior'),(2,'Forest Helper',150,15,1,0,100,'%s deals %s with %d projectile damage. Weapon: %s','null','null','%s uses skill: Connecting with Nature'),(3,'Magic Caster',100,25,1,0,100,'%s struck %s with %d lightning damage with spark. Weapon: %s','%s burns %s with %d fire damage with flame. Weapon: %s','%s pierced %s with %d magic damage with magic arrow. Weapon: %s','%s uses skill: Magic Amplifier'),(4,'Street Fighter',225,25,1,0,200,'%s hits %s and dealt %d direct damage with kick punch. Weapon: %s','%s hits %s and dealt %d direct damage with slam. Weapon: %s','%s hits %s and dealt %d direct damage with jump kick. Weapon: %s','%s uses skill: Known on the Street'),(5,'Sword Fighter',225,25,1,0,200,'null','null','null','%s uses skill: Sword Technique IV Water Dance'),(6,'Gladiator',225,25,1,0,200,'null','null','null','%s uses skill: Stronger Body'),(7,'Guardian of the Forest',250,20,1,0,200,'%s strikes %s with %d direct damage with greater roots. Weapon: %s','%s trembles %s with %d aoe damage with rumbling of the earth. Weapon: %s','%s strikes %s with %d direct damage with branch sword. Weapon: %s','%s uses skill: Defending the Forest'),(8,'Golem',250,20,1,0,200,'null','null','null','%s uses skill: Ancient Stone'),(9,'Forest Ranger',250,20,1,0,200,'null','null','null','%s uses skill: Essence of Live Arrows'),(10,'Arc Mage',200,30,1,0,200,'%s electrifies %s and deals %d lightning damage. Spell: Lightning Bolt. Weapon: %s','%s slashes %s and deals %d water damage spell water wave. Weapon: %s','%s burns %s full body with %d fire damage. Spell: Fire Flare. Weapon: %s','%s uses skill: False Data'),(11,'Scholar of the Academia',200,30,1,0,200,'null','null','null','%s uses skill: Fo